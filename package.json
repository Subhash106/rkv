{
  "name": "react-setup",
  "version": "1.0.0",
  "description": "React setup from scartch",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run sw:start\" \"webpack-dev-server --config webpack.dev.js --mode development --open\"",
    "test": "jest",
    "test-commit": "jest --coverage",
    "test-update": "jest --updateSnapshot",
    "build": "concurrently \"npm run sw:build\" \"webpack --config webpack.prod.js --mode production\"",
    "doc:dev": "mkdocs serve -f documentation/mkdocs.yml",
    "generate-sw": "workbox inject:manifest",
    "sw:start": "webpack --config public/webpack.config.js --watch",
    "sw:build": "webpack --config public/webpack.config.js"
  },
  "author": "Subhash Chandra",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^10.4.12",
    "babel-loader": "^8.0.2",
    "css-loader": "^1.0.0",
    "eslint": "^8.23.1",
    "eslint-plugin-react": "^7.31.8",
    "postcss": "^8.4.16",
    "postcss-import": "^15.0.0",
    "postcss-loader": "^4.0.4",
    "postcss-nested": "^5.0.6",
    "postcss-preset-env": "^7.8.2",
    "postcss-url": "^10.1.3",
    "style-loader": "^0.23.0",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.8",
    "webpack-merge": "^5.9.0",
    "workbox-cli": "^2.1.3",
    "worker-loader": "^3.0.8"
  },
  "dependencies": {
    "@types/jest": "^29.1.0",
    "concurrently": "^7.4.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "formik": "^2.2.9",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.1.1",
    "localforage": "^1.10.0",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hot-loader": "^4.13.0",
    "react-router": "^6.4.1",
    "react-router-dom": "^6.4.1",
    "webpack-bundle-optimizer": "^1.0.2",
    "yup": "^0.32.11"
  }
}
